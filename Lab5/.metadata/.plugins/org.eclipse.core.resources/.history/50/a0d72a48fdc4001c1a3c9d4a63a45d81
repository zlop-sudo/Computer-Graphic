package graphic;

public class Test {

  c.camera.initial(new double[] {5, -5, 0}, new double[] {0, 0, 2}, new double[] {0, 0, 1},1, 2, 20, 0);
  c.g.reset("knight.d.txt");
  c.illu.initial(new double[] {1, 1, 1}, 1, 0.3, 0.5, 0.7, 1, 8);
  c.constant_draw();
  c.gouraud_draw();
  c.phong_draw();

  c.camera.initial(new double[] {5, 0, 0}, new double[] {0, 0, 0}, new double[] {0, 0, 1},
          0.75, 2, 20, 0);
  c.g.reset("better-ball.d.txt");
  c.illu.initial(new double[] {-1, 1, -1}, 1, 0.3, 0.5, 0.7, 1, 8);
  c.constant_draw();
  c.gouraud_draw();
  c.phong_draw();
}
